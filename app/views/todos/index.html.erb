<p style="color: green"><%= notice %></p>

<% content_for :title, "Todos" %>

<h1>Todos</h1>

<div id="todos">
  <% @todos.each do |todo| %>
    <%= render todo %>
    <p>

      <%= link_to "Show this todo", todo, class: "btn btn-primary" %>

    </p>
  <% end %>
</div>

<%= link_to "New todo", new_todo_path, class: "btn btn-success" %>

